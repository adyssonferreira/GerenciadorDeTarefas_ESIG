{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/gerenciador-tarefas/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.js)",
            "dest": "/$1",
            "headers": {
                "Content-Type": "application/javascript",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.css)",
            "dest": "/$1",
            "headers": {
                "Content-Type": "text/css",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.woff2)",
            "dest": "/$1",
            "headers": {
                "Content-Type": "font/woff2",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.woff)",
            "dest": "/$1",
            "headers": {
                "Content-Type": "font/woff",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.ttf)",
            "dest": "/$1",
            "headers": {
                "Content-Type": "font/ttf",
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.svg|.*\\.ico)",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "headers": {
                "Content-Type": "text/html"
            }
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist/gerenciador-tarefas/browser",
    "installCommand": "npm install"
}
